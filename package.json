{
  "name": "@auric/radio",
  "version": "1.3.0",
  "description": "An npm module that uses radio-browser to play a radio with discord.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "keywords": [
    "@auric/radio",
    "discord.js",
    "discord",
    "radio-browser",
    "radio"
  ],
  "author": "Auric",
  "license": "UNLICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsauric/radio.git"
  },
  "dependencies": {
    "@discordjs/voice": "^0.14.0",
    "discord.js": "^14.7.1",
    "prism-media": "^1.3.4",
    "radio-browser": "^2.2.3"
  },
  "devDependencies": {
    "@sapphire/eslint-config": "4.3.8",
    "@sapphire/prettier-config": "1.4.5",
    "@sapphire/ts-config": "3.3.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.35",
    "@types/eslint": "8.21.1",
    "@types/node": "18.13.0",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.4",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "swc src -d dist",
    "declare": "tsc --emitDeclarationOnly --sourceMap --declarationMap --declaration",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "prettier": "@sapphire/prettier-config"
}
